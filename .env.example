# Application
API_V1_STR=/api/v1
PROJECT_NAME=JourneyAI
SECRET_KEY=your-secret-key
ACCESS_TOKEN_EXPIRE_MINUTES=11520
ENVIRONMENT=development

# MongoDB
MONGODB_URL=mongodb+srv://<username>:<password>@<cluster-url>/?retryWrites=true&w=majority&appName=smartsales
MONGODB_DB_NAME=journeyai

# Qdrant
QDRANT_URL=https://<qdrant-url>
QDRANT_API_KEY=your-qdrant-api-key

# Redis
REDIS_HOST=<redis-host>
REDIS_PORT=18871
REDIS_DB=0
REDIS_PASSWORD=your-redis-password
REDIS_URL=redis://default:your-redis-password@<redis-host>:18871

# External APIs
OPENAI_API_KEY=your-openai-api-key